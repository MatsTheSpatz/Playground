@model IEnumerable<Tuple<int,string>>
@{
    ViewBag.Title = "Start Page";
}


<h2>@ViewBag.Message</h2>

<div>
    <ul id="recipeList">
        @foreach  (var recipe in @Model)
        {
            <li>                
                @Html.ActionLink("ID" + recipe.Item1, "RecipeEditor", "Home", new { recipeId = recipe.Item1 }, null)
                @(recipe.Item2)
            </li>
        }
    </ul>
</div>



